41         {\cf19 return} {\cf17 false};\par
42     \par
43     {\cf20 // verifica o primeiro digito verificador}\par
44     {\cf18 int} soma = 0;\par
45     {\cf19 for}({\cf18 int} i = 0; i < 9; i++)\par
46         soma += (cpf[i] - {\cf23 '0'}) * (10 - i);\par
47     \par
48     {\cf18 int} resto = (soma * 10) % 11;\par
49     {\cf19 if} (resto == 10 || resto == 11)\par
